package main

import "fmt"

func main() {
}

func mult(x, y int) {

	fmt.Println(x * y)
}

func max(x int, y int) int {
	if x >= y {
		return x
	} else {
		return y
	}
}

func isEven(x int) bool {
	if x%2 == 0 {
		return true
	} else {
		return false
	}
}

func squareSumm(a, b int) int {
	var sum int
	for i := a; i <= b; i++ {
		sum += i * i
	}
	return (sum)
}

func oneOrTwo(x int, y int, s string) (int, string) {
	if s == "one" {
		return x, s
	} else if s == "two" {
		return y, s
	} else {
		return 0, s
	}
}

func marsAge(x int) int {
	return x * 365 / 687
}
